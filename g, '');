      // Generate content with OpenRouter (using Claude)
      const completion = await openai.chat.completions.create({
        model: "anthropic/claude-3-haiku",
        messages: [
          {
            role: "user",
            content: prompt,
          },
        ],
        temperature: 0.7,
      });
      
      const responseText = completion.choices[0].message.content || "[]";
      
